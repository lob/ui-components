{
  "extends": [
    "lob/browser",
    "plugin:vue/recommended"
  ],
  "globals": {
    "URLSearchParams": false
  },
  "parserOptions": {
    "ecmaVersion": 2018
  },
  "rules": {
    "lob/align-equals": 0,
    "no-extra-parens": 0,
    "no-lonely-if": 0,
    "no-redeclare": 0,
    "no-shadow": 0,
    "no-var": 0,
    "prefer-arrow-callback": 0,
    "prefer-reflect": 0,
    "prefer-template": 0
  }
}
